+<?xml version="1.0" encoding="UTF-8"?>
+<web-app xmlns="http://java.sun.com/xml/ns/javaee"
+         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
+         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee;
+          http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
+    version="3.1">
	<display-name>Examen Servlet</display-name>

	<servlet>
		<servlet-name>AltaConsolaServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.AltaConsolaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AltaConsolaServlet</servlet-name>
		<url-pattern>/consoleRegister</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>AltaConsola.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>AltaVideojuegosServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.AltaVideojuegosServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AltaVideojuegosServlet</servlet-name>
		<url-pattern>/gameRegister</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>AltaVideojuego.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ListadoConsolaServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListadoConsolaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListadoConsolaServlet</servlet-name>
		<url-pattern>consoleList</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>ListadoConsola.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ListadoVideojuegoServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListadoVideojuegoServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListadoVideojuegoServlet</servlet-name>
		<url-pattern>gamesList</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>ListadoVideojuegos.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ListadoConsolaMarcaServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListadoConsolaMarcaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListadoConsolaMarcaServlet</servlet-name>
		<url-pattern>markConsoleList</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>ListadoConsolasMarcas.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>ListadoVideojuegosMarcaServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListadoVideojuegosMarcaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ListadoVideojuegosMarcaServlet</servlet-name>
		<url-pattern>markGamesList</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>ListadoVideojuegosMarcas.jsp</welcome-file>
	</welcome-file-list>

</web-app>